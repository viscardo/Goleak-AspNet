@if (Session["UserLogged"] == null)
{


}
else
{
    var usuario = ((Goleak.Infra.Models.User)Session["UserLogged"]);

    <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
        <img src="http://graph.facebook.com/@usuario.Fb/picture?type=small" width="25px" height="25px" alt="" class="img-circle" />
        @usuario.FirstName @usuario.LastName <b class="caret"></b></a>
        <ul class="dropdown-menu">
            <li><a href="@Url.Content("~/User/Profile/" + usuario.UserLink)">My Profile</a></li>
            <li>@Html.Action("UpdateExternalLoginsList", "Account", new { ReturnUrl = ViewBag.ReturnUrl })</li>
            <li><a href="@Url.Content("~/User/RemoveProfile/" + usuario.UserLink)">Remove my profile</a></li>
            <li><a id="signOut" href="@Url.Content("~/Home/LogOut/")"> Sign Out</a></li>
        </ul>
    </li>
}